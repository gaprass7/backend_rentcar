        "/cars": {
            "get": {
                "tags": [
                    "Cars"
                ],
                "summary": "Get list of cars",
                "operationId": "35a83fd1dd6f38f7802c9474944713c6",
                "responses": {
                    "200": {
                        "description": "List of cars"
                    }
                }
            },
            "post": {
                "tags": [
                    "Cars"
                ],
                "summary": "Create new car",
                "operationId": "7efaf16ec0b383e6c0032d761f953fed",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "category_id",
                                    "brand",
                                    "model",
                                    "license_plate",
                                    "transmission",
                                    "year",
                                    "daily_rate",
                                    "passenger_capacity",
                                    "fuel_type"
                                ],
                                "properties": {
                                    "category_id": {
                                        "description": "ID kategori mobil",
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "brand": {
                                        "type": "string",
                                        "example": "Toyota"
                                    },
                                    "model": {
                                        "type": "string",
                                        "example": "Avanza"
                                    },
                                    "license_plate": {
                                        "type": "string",
                                        "example": "B1234XYZ"
                                    },
                                    "transmission": {
                                        "type": "string",
                                        "enum": [
                                            "automatic",
                                            "manual"
                                        ]
                                    },
                                    "year": {
                                        "type": "integer",
                                        "example": 2022
                                    },
                                    "daily_rate": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 500000
                                    },
                                    "passenger_capacity": {
                                        "type": "integer",
                                        "example": 7
                                    },
                                    "fuel_type": {
                                        "type": "string",
                                        "enum": [
                                            "gasoline",
                                            "diesel",
                                            "electric"
                                        ]
                                    },
                                    "status": {
                                        "type": "string",
                                        "enum": [
                                            "available",
                                            "rented",
                                            "maintenance"
                                        ],
                                        "example": "available"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Mobil nyaman untuk keluarga"
                                    },
                                    "penalty_rate_per_day": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 100000
                                    },
                                    "image": {
                                        "description": "Upload image file (jpg, jpeg, png, webp)",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Car created successfully"
                    }
                }
            }
        },
